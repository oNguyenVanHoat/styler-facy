        .box-infomation
          .title-facy-infomation 口座管理基本情報
          form.form-customize.registration-form__infor
            .box-item-block.info-facy
              .row
                .col-lg-3.col-md-4.col-sm-5.col-lg-offset-3.col-md-offset-2.col-sm-offset-1
                  .field-wrap
                    +input-field('氏名', '必須', '', 'スタイラー太郎', '', '', '', '', '17/40')
                .col-lg-3.col-md-4.col-sm-5
                  .field-wrap
                    +input-field('氏名（カタカナ）', '必須', '', 'スタイラータロウ', '', '', '', '', '17/40')
              .row
                .col-lg-3.col-md-4.col-sm-5.col-lg-offset-3.col-md-offset-2.col-sm-offset-1
                  .no-notice
                    +selectBox('都道府県', '必須', '男', '', '', '女')
              .row
                .col-lg-3.col-md-4.col-sm-5.col-lg-offset-3.col-md-offset-2.col-sm-offset-1
                  .no-notice.field-birthday
                    p.field-birthday__title 生年月日（西暦)
                    .field-birthday__content
                      +input-field('', '', '', '1990', '', '')
                      span.field-birthday__space.field-birthday__space--left /
                      +input-field('', '', '', '01', '', '')
                      span.field-birthday__space.field-birthday__space--right /
                      +input-field('', '', '', '01', '', '')
              .row
                .col-lg-3.col-md-4.col-sm-5.col-lg-offset-3.col-md-offset-2.col-sm-offset-1
                  .field-wrap
                    +input-field('郵便番号', '必須', '', '1234567', '', '', '', '', '9/12')
              .row
                .col-lg-3.col-md-4.col-sm-5.col-lg-offset-3.col-md-offset-2.col-sm-offset-1
                  .no-notice
                    +selectBox('都道府県', '必須', '東京都', '', '', '東京都2', '東京都3')
              .row
                .col-lg-3.col-md-4.col-sm-5.col-lg-offset-3.col-md-offset-2.col-sm-offset-1
                  .field-wrap
                    +input-field('市区', '必須', '', '渋谷区', '', '', '', '', '17/40')
