            h3.u-mb20 Form
            .row
              .col-md-12.lead
                .form-customize
                  +input-field('郵便番号', '必須', '', '1500041', '', '', '名前に誤りがあります。')
                  +selectBox('都道府県', '必須', '北海道', '', '都道府県誤りがあります。', 'Option first', 'Option second')
          .well.bg-white
            h3.u-mb20 Label
            .row
              .col-md-12.lead
                span.label-primary.label-primary--sm.label-primary--gentleman メンズ
                span.label-primary.label-primary--sm.label-primary--lady レディーズ
                span.label-primary.label-primary--md.label-primary--gray ニット・スウェット
          .well.bg-white
            h3.u-mb20 Title
