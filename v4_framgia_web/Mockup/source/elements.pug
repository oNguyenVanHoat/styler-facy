              +checkbox-custom('read-only-third', 'read-only-third', '利用規約に同意する', '', '利用規約に同意する')(class='checkbox-terms')
            +checkbox-custom('read-only', 'read-only', '未読のみ', '', '未読のみ')
          .well.bg-white
            h3.u-mb20 Button
            .row
              .col-md-12.lead
                +button-icon('変更', '')(class='button--primary')
