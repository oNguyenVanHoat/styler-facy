                  .field-wrap
                    +input-field('市区（カタカナ）', '必須', '', 'シブヤク', '', '', '', '', '17/40')
              .row
                .col-lg-3.col-md-4.col-sm-5.col-lg-offset-3.col-md-offset-2.col-sm-offset-1
                  .field-wrap
                    +input-field('町村', '必須', '', '神南', '', '', '', '', '17/40')
                .col-lg-3.col-md-4.col-sm-5
                  .field-wrap
                    +input-field('町村（カタカナ）', '必須', '', 'シブヤク', '', '', '', '', '17/40')
              .row
                .col-lg-3.col-md-4.col-sm-5.col-lg-offset-3.col-md-offset-2.col-sm-offset-1
                  .field-wrap
                    +input-field('丁目番地号', '必須', '', '１丁目１０番地６号', '', '', '', '', '17/40')
                .col-lg-3.col-md-4.col-sm-5
                  .field-wrap
                    +input-field('丁目番地号（カタカナ）', '必須', '', 'イッチョウメジュウバンチロクゴウ', '', '', '', '', '17/40')
              .row
                .col-lg-3.col-md-4.col-sm-5.col-lg-offset-3.col-md-offset-2.col-sm-offset-1
                  .field-wrap
                    +input-field('建物情報', '必須', '', 'バルビゾン９８　４０１', '', '', '', '', '17/40')
                .col-lg-3.col-md-4.col-sm-5
                  .field-wrap
                    +input-field('建物情報（カタカナ）', '必須', '', 'バルビゾンキュウジュウハチ　ヨンゼロイチ', '', '', '', '', '17/40')
              .row
                .col-lg-3.col-md-4.col-sm-5.col-lg-offset-3.col-md-offset-2.col-sm-offset-1
                  .field-wrap
                    +input-field('メールアドレス ', '必須', '', 'info@styler.link', '', '', '', '', '17/40')
              .row
                .col-lg-3.col-md-4.col-sm-5.col-lg-offset-3.col-md-offset-2.col-sm-offset-1
                  .field-wrap
                    +input-field('電話番号', '必須', '', '1234567', '', '', '', '', '17/40')
              .row
                .col-lg-3.col-md-4.col-sm-5.col-lg-offset-3.col-md-offset-2.col-sm-offset-1
                  .field-wrap
                    label ショップロゴ
                      span 必須
                    .bank-account__change--image.title-field
                      img(src='images/shop-account.png' alt='')
                    .button-wrapper
                      +button-icon('画像をアップロードする', 'sty-icon-photo registration-form__button-icon')(class='button--primary button--w250')
              button.btn.button.button--primary.button--primary-md.bank-account__button--change(type='button') 変更
              .registration-form__button-group
                +button('キャンセル')(class='button--cancel button--w270')
                +button-icon('保存する', '')(class='button--primary button--w270' data-toggle='modal' data-target='#modal-new-shop-registration')
